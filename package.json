{
  "name": "e2e-agent-web",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "start": "bun run src/server/index.ts",
    "dev": "bun --watch run src/server/index.ts",
    "test": "bun test",
    "test:bun": "bun test src/server/__tests__",
    "test:cli": "./tests/test_e2e_cli.sh && ./tests/test_e2e_functions.sh",
    "test:all": "bun test && ./tests/test_e2e_cli.sh && ./tests/test_e2e_functions.sh",
    "typecheck": "tsc --noEmit",
    "lint": "bunx biome check src/",
    "lint:fix": "bunx biome check --write src/",
    "format": "bunx biome format --write src/",
    "agent": "bun run src/agent/index.ts",
    "agent:help": "bun run src/agent/index.ts --help"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
